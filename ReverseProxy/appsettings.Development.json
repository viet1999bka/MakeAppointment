{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "users-route": {
        "ClusterId": "appointAPI-cluster",
        "Match": {
          "Path": "appointAPI/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      },
      "calendarAPI-route": {
        "ClusterId": "calendarAPI-cluster",
        "Match": {
          "Path": "calendarAPI/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]  
      },
      "webhookAPI-route": {
        "ClusterId": "webhookAPI-cluster",
        "Match": {
          "Path": "webhookAPI/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      }
    },
    "Clusters": {
      "appointAPI-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:7238/"
          }
        }
      },
      "webhookAPI-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:7150/"
          }
        }
      },
      "calendarAPI-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:7180/"
          }
        }
      }
    }
  }
}
